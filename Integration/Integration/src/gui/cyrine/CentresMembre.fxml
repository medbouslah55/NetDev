<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.cyrine.CentresMembreController">
   <children>
      <TableView fx:id="liste" layoutX="38.0" layoutY="96.0" prefHeight="356.0" prefWidth="631.0">
         <columns>
            <TableColumn fx:id="colNom" prefWidth="94.0" text="Nom centre" />
            <TableColumn fx:id="colTel" prefWidth="91.0" text="Telephone" />
            <TableColumn fx:id="colMail" prefWidth="182.0" text="Mail" />
            <TableColumn fx:id="colAdr" prefWidth="169.0" text="Adresse" />
            <TableColumn fx:id="colType" prefWidth="95.0" text="Type" />
         </columns>
      </TableView>
      <Label layoutX="28.0" layoutY="19.0" prefHeight="29.0" prefWidth="257.0" text="Affichage des centres" underline="true">
         <font>
            <Font name="Arial Black" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="ftRechNom" layoutX="38.0" layoutY="55.0" onAction="#RechParNom" promptText="Recherche par nom" />
      <Button fx:id="triCentre" layoutX="597.0" layoutY="55.0" mnemonicParsing="false" onAction="#TriListe" prefHeight="31.0" prefWidth="70.0" style="-fx-background-color: #f9bf3b; -fx-background-radius: 30px;" text="Tri (Nom) ↓" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="9.0" />
         </font>
      </Button>
      <Button fx:id="btnAct" layoutX="559.0" layoutY="456.0" mnemonicParsing="false" onAction="#listerAct" prefHeight="40.0" prefWidth="110.0" style="-fx-background-color: green; -fx-background-radius: 30px;" text="Activtés" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
